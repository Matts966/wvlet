-- just scan a table
from person

-- limit
from person
limit 5

-- sorting
from person
order by id

--- ascending order
from person
order by id asc

-- sort & limit
from person
order by time desc
limit 10


-- limit & sort
from person
limit 10
order by time asc


from person
transform
  -- Add a new column for aggregation
  age_group: age / 10
group by age_group
